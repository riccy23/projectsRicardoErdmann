void test() {
    local Integer[][] a;
    local Integer i;
    local Integer j;

    a = new(Integer[1000][]);
    i = 0;

    while (i < 1000) {
        a[i] = new(Integer[20]);
        j = 0;
        while (j < 20) {
            a[i][j]=i*1000+j;
            j = j + 1;
        }
        i = i + 1;
    }
}

